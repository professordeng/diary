---
---
[
  {% for page in site.pages %}
    {% if page.url.size == 16 %}
      {
        "url"      : {{ page.url | relative_url | jsonify }},
        "content"  : {{ page.content | strip_html | strip_newlines | remove: '  ' | jsonify }}
      } {% unless forloop.last %},{% endunless %}
    {% endif %}
  {% endfor %}
]
